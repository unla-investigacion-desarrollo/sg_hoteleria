<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.facturacion.Moneda" table="moneda">
	<id column="idMoneda" name="idMoneda">
		<generator class="identity"/>
	</id>
	
	<property column="codigo" name="codigo" type="string"/>
	<property column="descripcion" name="descripcion" type="string"/>
	<property column="cotizacion" name="cotizacion" type="double"/>
	
</class>
</hibernate-mapping>
