<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.facturacion.FormaDePago" table="formadepago">
	<id column="idFormaDePago" name="idFormaDePago">
		<generator class="identity"/>
	</id>
	
	<property column="codigo" name="codigo" type="int"/>
	<property column="descripcion" name="descripcion" type="string"/>
	
</class>
</hibernate-mapping>
