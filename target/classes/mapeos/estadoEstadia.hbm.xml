<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.estadia.EstadoEstadia" table="estadoestadia">
	<id column="idEstadoEstadia" name="idEstadoEstadia">
		<generator class="identity"/>
	</id>
	<property column="estado" name="estado" type="string"/>
	<property column="color" name="color" type="string"/>

</class>
</hibernate-mapping>
