<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.hotel.TipoHabitacion" table="tipohabitacion">
	<id column="idTipoHabitacion" name="idTipoHabitacion">
		<generator class="identity"/>
	</id>
	<property column="nombreTipoHabitacion" name="nombreTipoHabitacion" type="string"/>
	<property column="descripcion" name="descripcion" type="string"/>
	<property column="capacidad" name="capacidad" type="int"/>
</class>
</hibernate-mapping>