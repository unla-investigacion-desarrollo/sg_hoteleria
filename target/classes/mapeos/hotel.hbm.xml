<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.hotel.Hotel" table="hotel">
	<id column="idHotel" name="idHotel">
		<generator class="identity"/>
	</id>

	<property column="nombreHotel" name="nombreHotel" type="string"/>
	<property column="direccion" name="direccion" type="string"/>
	<property column="telefono" name="telefono" type="integer"/>
	<property column="email" name="email" type="string"/>
	<property column="paginaWeb" name="paginaWeb" type="string"/>


</class>
</hibernate-mapping>
