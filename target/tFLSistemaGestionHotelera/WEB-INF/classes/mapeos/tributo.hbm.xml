<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.facturacion.Tributo" table="tributo">
	<id column="idTributo" name="idTributo">
		<generator class="identity"/>
	</id>
	
	<property column="descripcion" name="descripcion" type="string"/>
	<property column="alicuota" name="alicuota" type="double"/>
	<property column="baseImponible" name="baseImponible" type="double"/>
	<property column="importe" name="importe" type="double"/>

</class>
</hibernate-mapping>
