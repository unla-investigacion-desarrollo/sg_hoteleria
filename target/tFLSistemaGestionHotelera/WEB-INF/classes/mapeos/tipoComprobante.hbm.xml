<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="sgh.mansilla.modelo.datos.facturacion.TipoComprobante" table="tipocomprobante">
	<id column="idTipoComprobante" name="idTipoComprobante">
		<generator class="identity"/>
	</id>
	
	<property column="codigo" name="codigo" type="string"/>
	<property column="descripcion" name="descripcion" type="string"/>

</class>
</hibernate-mapping>
